@model List<eco_edu_mvc.Models.Entities.Survey>

<h1>Survey List</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Topic</th>
            <th>Created Date</th>
            <th>End Date</th>
            <th>Created By</th>
            <th>Target Audience</th>
            <th>Active</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var survey in Model)
        {
            <tr>
                <td>@survey.SurveyId</td>
                <td>@survey.Title</td>
                <td>@survey.CreateDate</td>
                <td>@survey.EndDate</td>
                <td>@survey.CreatedBy</td>
                <td>@survey.TargetAudience</td>
                <td>@survey.Active</td>
                <td>
                    <a asp-action="Update" asp-route-id="@survey.SurveyId">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Post">Create New</a>