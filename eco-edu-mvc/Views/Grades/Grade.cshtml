@model eco_edu_mvc.Models.Entities.GradeTest

@{
    Layout = "_Layout";
}

<h2>Grade Entry</h2>

<h4>Competition Entry ID: @Model.EntryId</h4>

<form asp-action="Grade">
    <div class="form-group">
        <label asp-for="Score" class="control-label"></label>
        <input asp-for="Score" class="form-control" />
        <span asp-validation-for="Score" class="text-danger"></span>
    </div>
    <input type="hidden" asp-for="EntryId" />
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
